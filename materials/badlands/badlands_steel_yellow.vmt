LightmappedGeneric
{
	$basetexture "badlands/badlands_steel_yellow"
	$bumpmap "badlands/badlands_steel_ssbump"
	$surfaceprop Metal
	$ssbump 1

	// NOTE: Even though env_cubemap is here, it will turned off for DX6
	// because $multipass 1 isn't defined
	"$envmap" "env_cubemap"
	//"$envmaptint" "[ .56 .56 .75 ]"
	//"hdr?$envmaptint" "[ .09 .1 .12]"
	"$envmapcontrast" 1
	"$envmapsaturation" 1
	"$normalmapalphaenvmapmask" 1
	"$reflectivity" "[.23 .31 .31 ]" //this is a artificial radiosity boost

	%keywords "badlands"
}